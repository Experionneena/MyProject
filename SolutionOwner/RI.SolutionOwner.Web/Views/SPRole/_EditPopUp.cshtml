    }

    $("#ddlHierarchyIdEdit").change(function () {
        var hid = this.value;
        var rid = $("#hiddenId").val();
        $.ajax({
            url: '@Url.Action("ReloadFeaturePermissions", "SPRole")',
            type: 'POST',
            data: { id: rid, hierarchyId: hid },
            success: function (data) {
                if (data.Status == 1) {
                    $("#tblEditRole").html("");
                    $("#tblEditRole").html(data.Data);
                    if (data.IsActive == true) {
                        $("#isActiveCheckbox").prop('checked', true);
                    }
                    else {
                        $("#isActiveCheckbox").prop('checked', false);
                    }

                    for (var i = 0; i < data.checkedPermissions.length; i++) {
                        $("#" + data.checkedPermissions[i]).prop('checked', true);
                    }
                    var selectedHomePage = data.SelectedHomePage;
